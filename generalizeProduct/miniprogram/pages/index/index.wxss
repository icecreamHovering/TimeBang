
page {
    background: #f6f4f5;
}
.wrapper{
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    width: 100vw;
    height: 100vh;
}
.wrapper-block{
    flex-shrink: 0;
    width: 100%;
    height: 20rpx;
    background: #f6f4f5;
}
.wrapper-image{
    position: relative;
    flex-grow: 1;
    width: 100%;
    height: 520rpx;
    display: flex
}
.wrapper-image-item{
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    margin:20rpx;
    width: 710rpx;
    height: 100%;
}
.wrapper-line{
    flex-shrink: 0;
    margin-top: 50rpx;
    margin-bottom: 20rpx;
    width: 100%;
    text-align: center;
    font-size: 28rpx;
    font-weight: bold;
}
.wrapper-swipe{
    flex-grow: 2;
    height: 500rpx;
}
.wrapper-swipe-item{
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
}
.wrapper-swipe-item-wrap{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 460rpx;
}
.wrapper-swipe-item-wrap-a{
    height: 100%;
}
.wrapper-swipe-item-wrap-image{
    width: 95%;
    color: #f1f1f1;
    position: relative;
    height: 100%;
    background-size:100%;
}
.item-title {
  position: absolute;
  left: 40rpx;
  font-size: 40rpx;
  font-weight: bold;
  top: 70%;
}
.wrapper-movable{
    width: 120rpx;
    height: 120rpx;
    border-radius: 100%;
    background: rgba(0,0,0,.8);
}